class Solution {
    public String removeDuplicates(String s) {
        StringBuilder ss = new StringBuilder();
        for(int i = 0 ; i < s.length();i++)
        {
            char c = s.charAt(i);
            if(ss.length()> 0 && ss.charAt(ss.length()-1)==c)
            {
                ss.deleteCharAt(ss.length()-1);
            }
            else ss.append(c);
        }
        return ss.toString();
    }
}
